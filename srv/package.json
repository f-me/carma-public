{
  "private": true,
  "name": "carma",
  "version": "0.0.0",
  "description": "carma client",
  "main": "local.coffee",
  "engines": {
    "node": ">=6.7.0"
  },
  "scripts": {
    "clean-old-stuff": "rm -rf bower_components resources/static/{tpl,css,js,3p} resources/assets/react-components",
    "clean": "rm -rf resources/static/build/frontend",
    "build": "npm run webpack -- --progress --colors",
    "watch": "npm run webpack -- --progress --colors --watch",
    "prod-build": "env NODE_ENV=production npm run webpack -- --progress --colors",
    "clean-build": "npm run clean && npm run build",
    "prod-clean-build": "npm run clean && npm run prod-build",
    "clean-backend-templates": "rm -rf resources/static/build/backendPages",
    "build-backend-templates": "npm run pug -- resources/assets/template/backendPages/index.pug --extension=tpl --out resources/static/build/backendPages --pretty && npm run pug -- resources/assets/template/backendPages/login.pug --extension=html --out resources/static/build/backendPages --pretty",
    "clean-build-backend-templates": "npm run clean-backend-templates && npm run build-backend-templates",
    "webpack": "webpack",
    "pug": "pug"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/f-me/carma"
  },
  "keywords": [
    "best-ever-made-crm-with-haskell-inside"
  ],
  "author": "",
  "license": "",
  "bugs": {
    "url": "https://github.com/f-me/carma/issues"
  },
  "homepage": "https://github.com/f-me/carma",
  "devDependencies": {
    "coffee-loader": "^0.9.0",
    "coffeescript": "^1.12.7",
    "css-loader": "^0.28.7",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "imports-loader": "^0.7.1",
    "json-loader": "^0.5.7",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "loader-utils": "^1.1.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "pug-cli": "^1.0.0-alpha6",
    "pug-lint-config-clock": "^2.0.0",
    "pug-lint-loader": "0.0.4",
    "pug-loader": "^2.3.0",
    "pug-runtime": "^2.0.3",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.9.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "blueimp-md5": "~1.1.0",
    "bootstrap": "~3.3.0",
    "bootstrap-datepicker": "^1.7.1",
    "bootstrap-daterangepicker": "github:dangrossman/bootstrap-daterangepicker#v1.3.23",
    "bootstrap3-wysihtml5-bower": "~0.3.3",
    "d3": "^4.12.0",
    "datatables": "github:DataTables/DataTables#1.10.15",
    "finchjs": "~0.5.14",
    "immutable": "^4.0.0-rc.9",
    "jasny-bootstrap": "~3.1.0",
    "jquery-knob": "~1.2.11",
    "jquery-migrate": "^3.0.1",
    "jquery.browser": "~0.0.7",
    "js-base64": "~2.1.9",
    "knockout": "3.4.2",
    "moment": "~2.9.0",
    "moment-timezone": "~0.4.1",
    "mousetrap": "~1.5.2",
    "mustache": "~0.8.2",
    "normalize.css": "~3.0.0",
    "notify": "github:jpillora/notifyjs#0.3.2",
    "openlayers-2-build": "github:f-me/openlayers-2-build#v2.13.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "spin.js": "^2.3.2",
    "typeahead.js": "^0.11.1",
    "underscore": "~1.6.0",
    "whatwg-fetch": "^2.0.3",
    "wysihtml5x": "github:Voog/wysihtml#0.4.13"
  }
}
